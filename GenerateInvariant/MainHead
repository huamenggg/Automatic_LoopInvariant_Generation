    ofstream outFile;
    inFile.open(argv[1], ios::out | ios::in);
    outFile.open(argv[2], ios::out);
    if(!inFile) {
        cerr << "Can't open " << argv[1] << endl;
        exit(-1);
    }
    if(!outFile) {
        cerr << "Can't open " << argv[2] << endl;
        exit(-1);
    }

    string line;
    while(getline(inFile, line)) {
        vector<string> res = Split(line, " ");
        Node *p = new Node;
        // give Node variable
